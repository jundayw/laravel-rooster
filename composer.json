{
    "name": "nacosvel/laravel-rooster",
    "description": "Nacosvel Rooster Implementation for Laravel.",
    "keywords": [
        "Nacosvel Rooster Implementation for Laravel",
        "Nacosvel Rooster"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "nacosvel/feign": "^1.0",
        "nacosvel/rooster-contracts": "^1.0",
        "ramsey/uuid": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Nacosvel\\Rooster\\": "src/"
        }
    },
    "provide": {
        "nacosvel/rooster-implementation": "1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Nacosvel\\Rooster\\RoosterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
